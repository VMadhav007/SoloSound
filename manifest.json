{
    "manifest_version": 3,
    "name": "Audio Manager",
    "version": "1.0",
    "description": "Ensures only one tab plays audio at a time.",
    "permissions": [
      "tabs",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  